<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
  http://www.springframework.org/schema/context
  http://www.springframework.org/schema/context/spring-context-2.5.xsd">

 

  <context:annotation-config />

   <!-- Definition for profile bean -->
   <bean id = "beanid1" class = "com.spring.core.CoreModule.Details"></bean>

   <!-- Definition for student1 bean -->
   <bean id = "std" class = "com.spring.core.CoreModule.Student">
      <property name = "no" value = "111" />
      <property name = "name" value = "anu"/>
   </bean>

   <!-- Definition for student2 bean -->
   <bean id = "emp" class = "com.spring.core.CoreModule.Employee">
      <property name = "no" value = "222" />
      <property name = "name" value = "Nuha"/>
   </bean>

</beans>