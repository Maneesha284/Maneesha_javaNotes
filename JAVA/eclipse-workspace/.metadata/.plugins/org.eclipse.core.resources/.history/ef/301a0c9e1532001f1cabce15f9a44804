import java.util.Scanner;

public class ArrayNearestMinValue {
	
	static int getNearestMinValue(int[] min) {
		int nearestMinValue = 0;
		for(int i = 0; i < min.length; i++) {
			
			for(int j = i; j < min.length; j++) {
				
				if(min[i] >= min[j]) {
					int temp = min[i];
					min[i] = min[j];
					min[j] = temp;
				}
				
			}
			
		}
		nearestMinValue = --min[0];
		return nearestMinValue;
	}
	public static void main(String[] args) {
		
		Scanner sr = new Scanner(System.in);
		int min[] = new int[7];
		
		System.out.println("Enter elements in array");
		for(int i = 0; i < min.length; i++) {
			min[i] = sr.nextInt();
		}
		
		System.out.println("The Biggest element in array: " +getNearestMinValue(min));
		
	}
}
