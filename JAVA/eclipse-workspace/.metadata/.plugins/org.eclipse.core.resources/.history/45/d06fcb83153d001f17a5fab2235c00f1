package Now08072024;
@FunctionalInterface
interface LambdaExpInterf {
	void print();
}
@FunctionalInterface
interface LambdaExpInterfAdd {
	void add(int a,int b);
}
@FunctionalInterface
interface LambdaExpInterfReturn {
	int return(int a);
}
public class LambdaExp1	{
	public static void main(String[] args) {
		LambdaExpInterf name=()->System.out.println("maneesha");
		name.print();
		LambdaExpInterfAdd addobj=(a,b)->System.out.println(a+b);
		addobj.add(2, 3);
		LambdaExpInterfReturn rerurnobj=(a)->{retunr n*n};
		rerurnobj.return(2);
		
		
	}

}
