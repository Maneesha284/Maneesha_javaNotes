
public class Main3 {

	public static void main(String[] args) {
	
		i[0]=10;
		//i[1]=20;
		System.out.println(i[0]);
		class Inner{
			public String name;
			public Inner(String s) {
				name = s;
			}
		}
		Object obj=new Inner("InnerTest1");
		Inner t=(Inner)obj;
		System.out.println(t.name);

	}

}
